<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SimpleLongPressButton">
        <attr name="button_outsideCircleColor" format="color" />
        <attr name="button_insideCircleColor" format="color" />
        <attr name="button_rollingArcColor" format="color" />
        <attr name="button_textColor" format="color" />
        <attr name="button_textSize" format="dimension" />
        <attr name="button_text" format="string" />
